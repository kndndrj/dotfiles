! AltGr+Num is not a dead character
keycode  10 = 1 exclam 1 exclam asciitilde dead_tilde dead_tilde
keycode  11 = 2 quotedbl 2 quotedbl caron dead_caron dead_caron
keycode  12 = 3 numbersign 3 numbersign asciicircum dead_circumflex dead_circumflex
keycode  13 = 4 dollar 4 dollar breve dead_breve dead_breve
keycode  14 = 5 percent 5 percent degree dead_abovering dead_abovering
keycode  15 = 6 ampersand 6 ampersand ogonek dead_ogonek dead_ogonek
keycode  16 = 7 slash 7 slash grave dead_grave dead_grave
keycode  17 = 8 parenleft 8 parenleft abovedot dead_abovedot dead_abovedot
keycode  18 = 9 parenright 9 parenright apostrophe dead_acute dead_acute
keycode  19 = 0 equal 0 equal doubleacute dead_doubleacute dead_doubleacute
keycode  20 = apostrophe question apostrophe question diaeresis dead_diaeresis dead_diaeresis
keycode  21 = plus asterisk plus asterisk cedilla dead_cedilla dead_cedilla

! Set CapsLock to Escape
clear    lock
keycode  66 = Escape NoSymbol Escape

! Switch Alt and Super
remove mod1 = Alt_L
remove mod4 = Super_L
keycode 133 = Alt_L
keycode 64 = Super_L
add mod1 = Alt_L  
add mod4 = Super_L
